<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yu.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.yu.androidwidgets.TextViewActivity" />
        <activity android:name="com.example.yu.androidwidgets.BtnLearnActivity" />
        <activity android:name="com.example.yu.androidwidgets.TextEditActivity" />
        <activity android:name="com.example.yu.androidwidgets.ToggleButtonActivity" />
        <activity android:name="com.example.yu.androidwidgets.CheckBoxActivity" />
        <activity android:name="com.example.yu.androidwidgets.RadioButtonActivity" />
        <activity android:name="com.example.yu.androidwidgets.AlertDialogActivity" />
        <activity android:name="com.example.yu.androidwidgets.SpinnerActivity" />
        <activity android:name="com.example.yu.androidwidgets.ListViewActivity" />
        <activity android:name="com.example.yu.androidwidgets.CustomListViewTestActivity" />
        <activity android:name="com.example.yu.androidwidgets.RatingBarActivity" />
        <activity android:name="com.example.yu.androidwidgets.WebViewActivity" />
        <activity android:name="com.example.yu.androidwidgets.SeekBarActivity"></activity>
    </application>

</manifest>